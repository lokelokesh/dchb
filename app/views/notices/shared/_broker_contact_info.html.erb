	<tr>
		<td style="width: 15%"></td>
    <td style="width: 85%"><%= broker.try(:phone) %></td>
	</tr>
	<tr>
		<td style="width: 15%"></td>
    <td style="width: 85%"><%= broker.try(:email) %></td>
	</tr>
	<tr>
		<td style="width: 15%"></td>
    <td style="width: 85%"><%= broker.try(:web_address) %></td>
	</tr>
	<tr>
		<td style="width: 15%"></td>
		<td style="width: 85%"></td>
	</tr>
	<tr>
		<td style="width: 15%"></td>
		<td style="width: 85%"></td>
	</tr>
	<tr>
		<td style="width: 10%"></td>
		<td style="width: 90%">_________________________________________________________________________</td>
	</tr>
