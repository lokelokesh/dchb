<table style="width: 100%">
	<tr>
		<td style="width: 15%"><strong>Broker:&nbsp;&nbsp;&nbsp;&nbsp;</strong></td>
    <td style="width: 85%"><%= broker.primary_fullname.titleize %></td>
	</tr>
	<tr>
		<td style="width: 15%"></td>
    <td style="width: 85%"><%= broker.organization.titleize %></td>
	</tr>
	<tr>
		<td style="width: 15%"></td>
    <td style="width: 85%"><%= broker.address.try(:street_1) %></td>
	</tr>
	<tr>
		<td style="width: 15%"></td>
		<td style="width: 85%"><%= broker.address.try(:street_2) %></td>
	</tr>
